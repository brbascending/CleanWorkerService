{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Information",
        "Microsoft.EntityFrameworkCore": "Error"
      }
    }
  },
  "UseInMemoryDb": true,
  "DbConnectionString": "Data Source=WeatherReports.db;",
  "CooldownSettings": {
    "Cd": 1000
  },
  "WeatherReportSettings": {
    "MaxTemperature": 30,
    "MinTemperature": 20
  }
}
